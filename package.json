{
  "name": "insight-flow",
  "version": "1.0.0",
  "homepage": "/insight-flow/",
  "private": true,
  "dependencies": {
    "axios": "^1.8.4",
    "bootstrap-icons": "^1.11.3",
    "idb-keyval": "^6.2.1",
    "jwt-decode": "^4.0.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "resolve-url-loader": "^5.0.0",
    "styled-components": "^6.1.18",
    "webpack": "^5.98.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 firefox version",
      "last 1 chrome version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "react-app-rewired": "^2.2.1"
  }
}